{
  "version": 2,
  "name": "ip-system",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/admin",
      "dest": "/admin-panel.html"
    },
    {
      "src": "/user",
      "dest": "/user-panel.html"
    },
    {
      "src": "/",
      "dest": "/dashboard.html"
    }
  ]
}
